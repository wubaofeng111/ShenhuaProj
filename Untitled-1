<script type="text/javascript">
function toAddQualification(successUrl,errorUrl)
{
	//利用对话框返回的值（true 或者 false） 
    if(confirm("你不具备购买该产品的资质,或者资质已过期,点击确定提交相关资质,审核通过后再购买该产品.")) 
    {
		location.href=successUrl; 
	} 
    else 
    {
		location.href=errorUrl; 
	} 
}
function fmoney(s, n) {  
   n = n > 0 && n <= 20 ? n : 2;  
   s = parseFloat((s + "").replace(/[^\d\.-]/g, "")).toFixed(n) + "";  
   var l = s.split(".")[0].split("").reverse(),  
   r = s.split(".")[1];  
   t = "";  
   for(i = 0; i < l.length; i ++ )  
   {  
      t += l[i] + ((i + 1) % 3 == 0 && (i + 1) != l.length ? "" : "");  
   }  
   return t.split("").reverse().join("") + "." + r;  
}
$(function(){
	if(false) {
		setMenuPosition(3);
	} else {
		setMenuPosition(4);
	}
});

  $(document).ready(function(){
	  OnLoad();
	  var minDeposit=$("#minDeposit").val();
	  var checkedDeliveryPoints = $('input[type=checkbox][id$="_deliveryPointFlag"]:checked');
	  if(minDeposit!=0 && minDeposit!=null && minDeposit!=undefined){
		  if($("#depositFlag").html()!="percent" && $("#depositFlag").html()!="prodDeliveryPoint"){
			  if(checkedDeliveryPoints.size()==0){
				  $("#atg_store_depositAmount").val(Math.round(0));
			  }else{
				  $("#atg_store_depositAmount").val(Math.round(minDeposit));	
			  }  
			  
		  }
	  }
	  
	  
//根据数量变动更新显示总价
	  $("#atg_store_itemqty").keyup(function(){
		  	var total = $("#atg_store_itemqty").val()*294.0;
		  	total = fmoney(total,2);
			$("#orderTotal").html(total+'元');
		  });
//隐藏按钮防止重复提交  
	$("#atg_store_submit_waiting").hide();

	$("#atg_store_commitForm").submit(function(){
		if($("#atg_store_commitForm").validate().form()==true){
			raStatus=false;
			submitUserRaStatusForm();
			if(raStatus){
				raStatus=false;
				try{
					var profileDN=$("#profileDN").val();
					SelectCertificateOnClick(profileDN);//签名验签
				}catch(e){
				}
				if(errFlag){
					alert("证书校验失败");
					errFlag=false;//重置标志位
				    return false;//不再提交表单
				}
			}
			$("#atg_store_submit").hide();
			$("#atg_store_submit_waiting").attr("disabled","disabled");
			$("#atg_store_submit_waiting").show();			
	    }

	  	return true;
	  });
//修改表单恢复提交功能
	$("#atg_store_name").keyup(function(){
		$("#atg_store_submit_waiting").hide();
		$("#atg_store_submit").show();
	});
	$("#atg_store_phone").keyup(function(){
		$("#atg_store_submit_waiting").hide();
		$("#atg_store_submit").show();
	});
	$("#atg_store_addr").keyup(function(){
		$("#atg_store_submit_waiting").hide();
		$("#atg_store_submit").show();
	});
	$("#atg_store_itemqty").keyup(function(){
		$("#atg_store_submit_waiting").hide();
		$("#atg_store_submit").show();
	});
	$("#atg_store_depositAmount").keyup(function(){
		$("#atg_store_submit_waiting").hide();
		$("#atg_store_submit").show();
	});
//勾选启用提交按钮	  
		$("#atg_store_submit").attr("disabled","disabled");
		$("#atg_store_submit").removeClass("btn-primary");
		$("#atg_store_submit_check").click(function(){
			if($("#atg_store_submit_check").attr("checked")=="checked"){
				$("#atg_store_submit").removeAttr("disabled");
				$("#atg_store_submit").addClass("btn-primary")
			}else{
				$("#atg_store_submit").removeClass("btn-primary");
				$("#atg_store_submit").attr("disabled","disabled");
			}
		});
		
		$.validator.methods.checkDeposit = function(value, element, param) {
			var pattern = /^\d+\.?\d{0,2}$/;
	        return pattern.test(value);
	    };
		$.validator.methods.checkProdDeliveryPoint = function(value, element, param) {
			if($("#depositFlag").html()=="prodDeliveryPoint"){
				var minDeposit=$("#minDeposit").val();
				return value%minDeposit==0;
			}
	        return true;
	    };	    
	    
//表单验证		
		$("#atg_store_commitForm").validate({
			rules : {
				atg_store_companyName : {
					required : true,
					maxlength : 30
				},
				atg_store_name : {
					required : true,
					maxlength : 40
				},
				atg_store_phone : {
					required : true,
					phoneNumber2 : true,
					maxlength : 18
				},
				atg_store_addr : {
					required : true,
					maxlength : 256
				},
				atg_store_itemqty : {
					required : true,
					digits:true
				},
				atg_store_depositAmount : {
					required : true,
					min : 1,
					checkDeposit : true,
					checkProdDeliveryPoint : true
				}
			},
			messages : {
				atg_store_companyName : {
					required : "此项必填",
					maxlength : "长度不能超过30个字符"
				},
				atg_store_name : {
					required : "此项必填",
					maxlength : "长度不能超过40个字符"
				},
				atg_store_phone : {
					required : "此项必填",
					maxlength : "请输入正确格式的电话"
				},
				atg_store_addr : {
					required : "此项必填",
					maxlength : "长度不能超过256个字符"
				},
				atg_store_itemqty : {
					required : "数量必填",
					digits:"要求有效数字"
				},
				atg_store_depositAmount : {
					required : "数量必填",
					min:"总价不能为0",
					checkDeposit : "保证金不能多于2位小数",
					checkProdDeliveryPoint:"您输入的保证金的金额不符合要求，请重新输入！"
				}
				 
			}
		});
	});
  

  function checkDeliverySize(){
	  var checkedDeliveryPoints = $('input[type=checkbox][id$="_deliveryPointFlag"]:checked');
	  var depSize = checkedDeliveryPoints.size();
	  if(depSize==0){
		 alert("请选择交割地");
		 return false;
	  }else{
		 return true;
	  }
  }

  
  function accountDepPortFlag(deliveryPointId){
	  /*** replaced by sunjian 2014.09.10
	  var depId= deliveryPointId+"_deliveryPointFlag";
	  var deliPoint = document.getElementById(depId);
	  var deliveryPrice= $("#deliveryPointValue").val();
	  var deliveryPoints= $("#atg_store_depositDeliveryPoints").val();
	  var exsitDepositAmount = $("#deliveryPointDepositAmount").val();
	  if(exsitDepositAmount!=null && exsitDepositAmount!=''){
		  exsitDepositAmount = parseInt(exsitDepositAmount);
	  }
	  if(deliveryPrice!=null && deliveryPrice!=''){
		  deliveryPrice=parseInt(deliveryPrice);
	  }
	  if(deliPoint.checked){
		  var total = exsitDepositAmount+deliveryPrice;
		  	total = fmoney(total,2);
		  	$("#deliveryPointDepositAmount").val(total);
			$("#atg_store_depositAmount").val(total);
			deliveryPoints=deliveryPoints+"@"+deliveryPointId;
	  }else{
		  var total = exsitDepositAmount-deliveryPrice;
		  	total = fmoney(total,2);
		  	$("#deliveryPointDepositAmount").val(total);
			$("#atg_store_depositAmount").val(total);
			if(deliveryPoints!=null || deliveryPoints!=''){
				  var deliArr = new Array();
				  deliArr = deliveryPoints.split("@");
				  for(var i=0;i<deliArr.length;i++){
					  if(deliArr[i]==deliveryPointId){
						  deliArr[i]="";
					  }
				  }
			  }
			deliveryPoints="";
			for(var i=0;i<deliArr.length;i++){
				deliveryPoints=deliveryPoints+"@"+deliArr[i];
			  }
	  }
	  $("#atg_store_depositDeliveryPoints").val(deliveryPoints);
	  */
	  
	 var deliveryPrice= $("#deliveryPointValue").val();
	 var checkedDeliveryPoints = $('input[type=checkbox][id$="_deliveryPointFlag"]:checked');
	 var depAmount = checkedDeliveryPoints.size()*deliveryPrice;
	
	 depAmount = (depAmount == 0?deliveryPrice:depAmount); //如果没有选择交货地，默认是单价价格
	 depAmount =fmoney(depAmount,2); //格式化，保留两位小数
	 if(checkedDeliveryPoints.size()==0){
		depAmount=0;
	 }
		 
	 //alert("--------checkedDeliveryPoints.size()----------"+checkedDeliveryPoints.size()+"-------------depAmount----------"+depAmount);
	 $("#deliveryPointDepositAmount").val(depAmount);
	 $("#atg_store_depositAmount").val(depAmount);
	 var checkedDeliveryPointIdStr = "";
	 checkedDeliveryPoints.each(function(i,n){
		 var dpId = $(this).attr("id").split("_")[0];
		 checkedDeliveryPointIdStr +=  dpId;
		 if(i < (checkedDeliveryPoints.size()-1)){
			 checkedDeliveryPointIdStr += "@";
		 }
	 });
	 $("#atg_store_depositDeliveryPoints").val(checkedDeliveryPointIdStr);
 }
  
